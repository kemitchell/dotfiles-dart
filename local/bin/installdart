#!/bin/sh
sudo apt-get install apt-transport-https
sudo sh -c 'wget -qO- https://dl-ssl.google.com/linux/linux_signing_key.pub | apt-key add -'
APT="/etc/apt/source.list.d/dart.list"
if [ ! -f "$APT" ]; then
  sudo sh -c "wget -qO- https://storage.googleapis.com/download.dartlang.org/linux/debian/dart_stable.list > '$APT'/etc/apt/sources.list.d/dart_stable.list"
fi
sudo apt-get update
sudo apt-get install dart
